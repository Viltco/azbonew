<odoo>

    <record id="view_mrp_wizard_form" model="ir.ui.view">
		<field name="name">MRP Wizard</field>
		<field name="model">mrp.wizard</field>
		<field name="arch" type="xml">
			<form >
				<group>
					<group>
						<field name="quantity"/>
						<field name="uom_id"/>
						<field name="bom_id"/>
					</group>
				</group>
				<footer>
					<button name="action_create_mo" string="Create MO" type="object" class="btn-primary"/>
					<button  string="Cancel" type="object" class="btn-secondary" special="cancel"/>
				</footer>
			</form>
		</field>
	</record>

</odoo>